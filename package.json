{
  "name": "card-learner",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build-android": "npm run build && npm run sync && npm run assemble && npm run copy-apk",
    "sync": "npx cap sync",
    "assemble": "cd android && gradlew assembleDebug",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "resources": "cordova-res ios --skip-config --copy && cordova-res android --skip-config --copy",
    "copy-apk": "cross-var cp ./android/app/build/outputs/apk/debug/app-debug.apk \"./releases/apk/app-$npm_package_version.apk\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.1",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@capacitor/android": "^4.0.1",
    "@capacitor/core": "4.0.1",
    "@capacitor/ios": "^4.0.1",
    "adb": "^0.2.0",
    "dexie": "^3.2.2",
    "fs": "^0.0.1-security",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "~14.1.1",
    "@angular/compiler-cli": "^14.1.0",
    "@capacitor/cli": "4.0.1",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^18.6.5",
    "cordova-res": "^0.15.4",
    "cross-var": "^1.1.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
